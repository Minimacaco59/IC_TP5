pipeline {
    agent any
    stages {
        stage('Build') {
            steps {                             
                bat 'npm install'
                bat 'npm run build'
            }
        }
        stage('Test') {
            steps {
                bat 'npm test'
            }
        }
        stage('Packaging') {
            steps {
                bat 'npm pack'
            }
        }
        
    }
}
